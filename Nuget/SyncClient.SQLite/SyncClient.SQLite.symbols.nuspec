        <file src="..\..\C#\SyncClient.iOS\bin\iPhone\Debug\Microsoft.Synchronization.ClientServices.iOS.pdb" target="lib\Xamarin.iOS\Microsoft.Synchronization.ClientServices.iOS.pdb" />
        <file src="..\..\C#\SyncClient.Net\bin\Debug\Microsoft.Synchronization.ClientServices.Net.pdb" target="lib\net45\Microsoft.Synchronization.ClientServices.Net.pdb" />
        <file src="..\..\C#\SyncClient.Universal\bin\Debug\Microsoft.Synchronization.ClientServices.WinRT.pdb" target="lib\portable-windows8+net45\Microsoft.Synchronization.ClientServices.WinRT.pdb" />
        <file src="..\..\C#\WP8Client\bin\x86\Debug\Microsoft.Synchronization.ClientServices.WP8.pdb" target="lib\wp8\Microsoft.Synchronization.ClientServices.WP8.pdb" />
        <file src="..\..\C#\WP8Client\bin\x86\Debug\Microsoft.Synchronization.ClientServices.WP8.pdb" target="lib\wp81\Microsoft.Synchronization.ClientServices.WP8.pdb" />
        <file src="..\..\C#\SyncClient.Universal\bin\Debug\Microsoft.Synchronization.ClientServices.WinRT.pdb" target="lib\wpa81\Microsoft.Synchronization.ClientServices.WinRT.pdb" />
		
		
		<file src="..\..\C#\SyncClient.Shared\**\*.cs" target="src" />
		
		
        <file src="tools\SyncSvcUtil.exe" target="tools\SyncSvcUtil.exe" />
        <file src="tools\SyncSvcUtilUI.exe" target="tools\SyncSvcUtilUI.exe" />
